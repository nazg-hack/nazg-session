{
    "name": "nazg/hsession",
    "description": "PSR-7, PSR-11, PSR-15 compliant Action-Domain-Responder (ADR) Micro framework for HHVM/Hack",
    "type": "project",
    "keywords": [
      "hhvm",
      "hack",
      "session"
    ],
    "license": "MIT",
    "authors": [
      {
        "name": "Yuuki Takezawa",
        "email": "yuuki.takezawa@comnect.jp.net"
      }
    ],
    "require": {
      "hhvm": ">=3.25.0",
      "hhvm/hhvm-autoload": "^1.6",
      "nazg/hcache": "^0.1.2"
    },
    "require-dev": {
      "phpunit/phpunit": "^5.7",
      "91carriage/phpunit-hhi": "^5.7",
      "hhvm/hhast": "^1.0.1|^3.26.0"
    },
    "autoload": {
      "psr-4": {
        "Nazg\\HSession\\": "src/"
      }
    },
    "minimum-stability": "stable",
    "extra": {
      "branch-alias": {
        "dev-master": "1.x-dev"
      }
    },
    "config": {
      "preferred-install": {
        "*": "dist"
      },
      "sort-packages": true
    }
  }
